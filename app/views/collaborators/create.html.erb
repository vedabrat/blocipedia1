<h1>Add Collaborator</h1>

<%= form_for [@wiki] do |f| %>


  <%= collection_check_boxes(:wiki, :user_ids, User.all - [f.object.user], :id, :email) do |u| %>
    <div class="checkbox">
      <%= u.label do %>
        <%= u.check_box %>
        <%= u.object.email %>
      <% end %>
    </div>
  <% end %>

  <div class="form-group">
     <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>
